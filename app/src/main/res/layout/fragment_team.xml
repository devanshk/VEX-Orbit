                android:layout_marginLeft="10sp"
                android:textColor="@color/header_color"
                android:layout_toRightOf="@id/team_number_title"
                android:hint="@string/team_number_hint"
                android:textColorHint="@color/hint_color"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/team_name_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/team_name_title"
                android:textSize="35sp"
                android:layout_marginLeft="70sp"
                android:textColor="@color/header_color"/>
        <EditText
            android:id="@+id/team_name_field"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text=""
            android:textSize="35sp"
            android:layout_marginLeft="10sp"
            android:textColor="@color/header_color"
            android:layout_toRightOf="@id/team_name_title"
            android:hint="@string/team_name_hint"
            android:textColorHint="@color/hint_color"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
        <TextView
            android:id="@+id/team_club_title"
            android:layout_width="wrap_content"
