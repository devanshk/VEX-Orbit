<ScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@android:color/background_light"
    android:padding="0sp"
    android:id="@+id/teamScrollView">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:focusable="true"
        android:focusableInTouchMode="true">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="350dp"
                android:id="@+id/main_picture"
                android:layout_row="0"
                android:layout_column="0"
                android:background="@android:color/darker_gray"
                android:scaleType="centerCrop"
                android:src="@drawable/picture_placeholder_tvdpi"/>

            <ProgressBar
                android:id="@+id/main_progress_bar"
                android:layout_width="wrap_content"
                android:layout_height="350dp"
                android:layout_centerHorizontal="true"
                android:indeterminate="true"
                android:visibility="gone"/>

            <TextView
                android:id="@+id/main_picture_helper"
                android:layout_width="wrap_content"
                android:layout_height="350dp"
                android:text="@string/main_picture_helper_text"
                android:textColor="@android:color/white"
                android:textStyle="bold"
                android:textSize="30sp"
                android:gravity="center"
                android:layout_centerHorizontal="true"
                android:shadowColor="@android:color/black"
                android:shadowDx="1"
                android:shadowDy="1"
                android:shadowRadius="5"/>

            <TextView
                android:id="@+id/main_club_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textColor="@android:color/white"
                android:textSize="30sp"
                android:layout_alignBottom="@id/main_picture"
                android:layout_marginLeft="25sp"
                android:layout_marginBottom="10sp"
                android:shadowColor="@android:color/black"
                android:shadowDx="1"
                android:shadowDy="1"
                android:shadowRadius="5"/>
            <TextView
                android:id="@+id/main_team_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textColor="@android:color/white"
                android:textSize="30sp"
                android:layout_toRightOf="@id/main_club_name"
                android:layout_alignBottom="@id/main_picture"
                android:layout_marginLeft="10sp"
                android:layout_marginBottom="10sp"
                android:shadowColor="@android:color/black"
                android:shadowDx="1"
                android:shadowDy="1"
                android:shadowRadius="8"/>

            <TextView
                android:id="@+id/main_team_number"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_above="@id/main_team_name"
                android:text=""
                android:layout_marginLeft="25sp"
                android:textColor="@android:color/white"
                android:textSize="56dip"
                android:shadowColor="@android:color/black"
                android:shadowDx="1"
                android:shadowDy="1"
                android:shadowRadius="9"/>

            <TextView
                android:id="@+id/main_score"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignRight="@id/main_picture"
                android:text=""
                android:layout_alignBottom="@id/main_team_name"
                android:textColor="@android:color/white"
                android:textSize="100sp"
                android:layout_marginRight="25sp"
                android:shadowColor="@android:color/black"
                android:shadowDx="1"
                android:shadowDy="1"
                android:shadowRadius="15"/>

            <!--Removed Image Buttons
            <ImageButton
                android:id="@+id/settings_button"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:scaleType="fitXY"
                android:src="@drawable/ic_action_settings"
                android:background="#00ffffff"
                android:layout_alignRight="@id/main_picture"
                android:layout_marginRight="15dp"
                android:layout_marginTop="15dp"/>

            <ImageButton
                android:id="@+id/share_button"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:scaleType="fitXY"
                android:src="@drawable/ic_action_share"
                android:background="#00ffffff"
                android:layout_toLeftOf="@id/settings_button"
                android:layout_marginRight="15dp"
                android:layout_marginTop="15dp"/>

            <ImageButton
                android:id="@+id/edit_button"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:scaleType="fitXY"
                android:src="@drawable/ic_action_edit"
                android:background="#00ffffff"
                android:layout_marginRight="15dp"
                android:layout_marginTop="15dp"
                android:layout_toLeftOf="@id/share_button"/>
                -->

            <View
                android:layout_width="fill_parent"
                android:layout_height="5sp"
                android:background="@android:color/darker_gray"
                android:layout_below="@id/main_picture"
                android:layout_marginTop="10sp"
                android:layout_marginBottom="10sp"/>
        </RelativeLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/team_info_title"
