                android:layout_marginLeft="10dp"/>
            <TextView
                android:id="@+id/auton2_dynamic_reliability"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0%"
                android:textSize="35sp"
                android:layout_marginLeft="-30dp"
                android:layout_alignRight="@id/auton2_seek_bar"
                android:layout_alignBottom="@id/auton2_reliable_title"
                android:layout_centerVertical="true"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/auton_notes_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/notes_title"
                android:textSize="30sp"
                android:layout_marginLeft="40dp"
                android:textColor="@color/header_color"/>
            <EditText
                android:id="@+id/auton_notes"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/auton_notes_title"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="20dp"
                android:hint="@string/auton_notes_hint"
                android:textColorHint="@color/hint_color"
                android:textColor="@color/header_color"/>
        </RelativeLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="3sp"
            android:background="@android:color/darker_gray"
            android:layout_marginTop="10sp"
            android:layout_marginBottom="10sp"/>
        <TextView
            android:id="@+id/motor_config_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/motor_config_title"
            android:textSize="30sp"
            android:layout_marginLeft="40sp"
            android:textColor="@color/header_color"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginRight="50dp">
                <NumberPicker
                    android:id="@+id/drive_motors_field"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/motor_drive_title"
                    android:textSize="30sp"
                    android:textColor="@color/header_color"
                    android:layout_gravity="center_horizontal"/>
                </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginRight="50dp">
                <NumberPicker
                    android:id="@+id/lift_motors_field"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/motor_lift_title"
                    android:textSize="30sp"
                    android:textColor="@color/header_color"
                    android:layout_gravity="center_horizontal"/>
                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Pneumatic"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <NumberPicker
                    android:id="@+id/intake_motors_field"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/motor_intake_title"
                    android:textSize="30sp"
                    android:textColor="@color/header_color"
                    android:layout_gravity="center_horizontal"/>
                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Pneumatic"/>

            </LinearLayout>
        </LinearLayout>

        <View
            android:layout_width="475dp"
            android:layout_height="2sp"
            android:background="@android:color/darker_gray"
            android:layout_marginTop="10sp"
            android:layout_marginBottom="10sp"
            android:layout_gravity="center"/>

        <TextView
            android:id="@+id/delete_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp"
            android:layout_marginTop="20dp"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"
            android:text="@string/delete_team"
            android:textColor="@android:color/white"
            android:textSize="25sp"
            android:gravity="center"
            android:background="@color/delete_button_color"
            android:enabled="true"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/credits"
            android:textColor="@color/credits_color"
            android:layout_gravity="center"
            android:layout_marginTop="50dp"
            android:layout_marginBottom="20dp"/>

    </LinearLayout>
</ScrollView>